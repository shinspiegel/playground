; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Button",
"class": "ButtonExtra",
"language": "GDScript",
"path": "res://Screens/components/BaseButton.gd"
}, {
"base": "CanvasLayer",
"class": "GameManager",
"language": "GDScript",
"path": "res://Game/GameManager.gd"
}, {
"base": "Node2D",
"class": "LevelManager",
"language": "GDScript",
"path": "res://Game/LevelManager.gd"
}, {
"base": "Control",
"class": "ScreenManager",
"language": "GDScript",
"path": "res://Screens/ScreenManager.gd"
} ]
_global_script_class_icons={
"ButtonExtra": "",
"GameManager": "",
"LevelManager": "",
"ScreenManager": ""
}

[application]

config/name="Quick Vania Godot 3"
run/main_scene="res://Game/GameManager.tscn"
config/icon="res://icon.png"

[autoload]

Helper="*res://Globals/Helper.gd"

[display]

window/size/width=320
window/size/height=192
window/size/test_width=1280
window/size/test_height=768
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
