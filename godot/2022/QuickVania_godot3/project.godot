[application]

config/name="Quick Vania Godot 3"
run/main_scene="res://Game/GameManager.tscn"
config/icon="res://icon.png"

[autoload]

Manager="*res://Globals/Manager.gd"
KeysMap="*res://Globals/KeysMap.gd"
