; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Story Driven"
config/version="0.0.0"
run/main_scene="res://screens/start_screen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://singletons/game_manager.tscn"
SceneManager="*res://singletons/scene_manager.tscn"
AudioManager="*res://singletons/audio_manager.tscn"
PartyManager="*res://singletons/party_manager.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[layer_names]

2d_physics/layer_1="world"
2d_physics/layer_2="player"
2d_physics/layer_3="enemies"
2d_physics/layer_4="interactable"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
