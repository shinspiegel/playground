[application]

config/name="experiment_godot_4"
run/main_scene="res://Level.tscn"
config/features=PackedStringArray("4.0", "Vulkan Clustered")
config/icon="res://icon.png"

[autoload]

SignalBus="*res://Globals/SignalBus.gd"
Constants="*res://Globals/Constants.gd"

[display]

window/size/viewport_width=320
window/size/viewport_height=160
window/size/resizable=false
window/size/window_width_override=1280
window/size/window_height_override=640
window/stretch/mode="viewport"

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Level"
2d_physics/layer_3="Enemies"
2d_physics/layer_4="Hazards"
